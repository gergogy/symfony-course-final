<div id="jobs">
    {% for cname, cjobs in jobs %}
        <div class="category_{{ cname|lower }}">
            <div class="category">
                <h1><a href="#">{{ cname }}</a></h1>
            </div>
        </div>
        <table class="jobs">
            <tbody>
            {% for job in cjobs %}
                {{ include('@Front/FrontController/_job.html.twig', { job: job }) }}
            {% endfor %}
            </tbody>
        </table>
    {% endfor %}
</div>